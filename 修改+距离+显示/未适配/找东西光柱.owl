变量
{
	玩家:
		26: thise
}

规则("规则 50")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		全部;
	}

	条件
	{
		事件玩家 == 主机玩家;
	}

	动作
	{
		禁用 等待(5, 无视条件);
		事件玩家.thise = 1;
		While(事件玩家.thise <= 70);
			If(全局.A[事件玩家.thise] != 0);
				创建效果(事件玩家, 光柱, 水绿色, 全局.A[事件玩家.thise], 0.5, 无);
			End;
			事件玩家.thise += 1;
			等待(0.250, 无视条件);
		End;
	}
}