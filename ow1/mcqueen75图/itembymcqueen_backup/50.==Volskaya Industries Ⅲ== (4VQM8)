		事件玩家.E[7] = 上一个文本ID;
		禁用 根据条件跳过(全局.G < 8, 2);
		禁用 创建HUD文本(事件玩家, 字符串("{0} / {1}", 事件玩家.G[8], 全局.C[8]), 无, 自定义字符串("콜벨"), 左边, 8, 颜色(紫色), 颜色(白色), 颜色(紫色), 可见和字符串, 始终不可见);
		禁用 事件玩家.E[8] = 上一个文本ID;
		禁用 根据条件跳过(全局.G < 9, 4);
		禁用 创建HUD文本(事件玩家, 字符串("{0} / {1}", 事件玩家.G[9], 全局.C[9]), 无, 自定义字符串("싱글 아이템 :"), 左边, 9, 颜色(亮紫色), 颜色(亮紫色), 颜色(亮紫色), 可见和字符串, 始终不可见);
		禁用 事件玩家.E[9] = 上一个文本ID;
		禁用 创建HUD文本(事件玩家, 无, 自定义字符串(
			"하스스톤 게임 (빨강)\r\n'Mass Production' (주황)\r\n달 (노랑)\r\n에코가 처음 등장했던 곳 (초록) \r\n봉투 보관함 (파랑)\r\n5가지 색상으로 적힌 단어 (보라)\r\n'00:00:00' (분홍)"),
			无, 左边, 10, 颜色(玫红), 颜色(绿色), 颜色(玫红), 可见和字符串, 始终不可见);
		禁用 事件玩家.E[10] = 上一个文本ID;
		事件玩家.A = 0;
	}
}

规则("all items found (victory!)")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		全部;
	}

	条件
	{
		事件玩家.Q == 全局.O;
		已重生(事件玩家) == 真;
	}

	动作
	{
		设置不可见(事件玩家, 无);
		大字体信息(所有玩家(所有队伍), 字符串("{0} {1} {2}", 英雄图标字符串(所用英雄(事件玩家)), 事件玩家, 字符串("{0}！", 自定义字符串("클리어"))));
		播放效果(事件玩家, 环状爆炸声音, 颜色(白色), 事件玩家, 20);
		播放效果(事件玩家, 有益爆炸, 颜色(蓝色), 事件玩家, 10);
		播放效果(事件玩家, 环状爆炸, 颜色(白色), 事件玩家, 20);
		全局.H = 事件玩家;
		交流(事件玩家, 表情（上）);
		设置慢动作(30);
		等待(1.250, 无视条件);
		设置慢动作(100);
		等待(3, 无视条件);
		小字体信息(事件玩家, 字符串("{0} {1}", 自定义字符串("수고하셨습니다"), 字符串("{0}！", 事件玩家)));
		等待(15, 无视条件);
		播放效果(事件玩家, 状态爆炸声音, 颜色(白色), 事件玩家, 10);
		消除HUD文本(事件玩家.AA);
		等待(0.250, 无视条件);
		事件玩家.AA = 上一个文本ID;
	}
}

规则("player pressed \"F\" key on the empty place")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		全部;
	}

	条件
	{
		事件玩家.J == 真;
		具有状态(事件玩家, 击倒) == 假;
		事件玩家.Q != 全局.O;
	}

	动作
	{
		播放效果(事件玩家, 负面状态施加声音, 颜色(白色), 事件玩家, 50);
		设置状态(事件玩家, 无, 击倒, 1.500);
		小字体信息(事件玩家, 自定义字符串("이 물건이 아닙니다."));
		事件玩家.J = 假;
	}
}

规则("player is knocked down")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		全部;
	}

	条件
	{
		事件玩家.J == 真;
	}

	动作
	{
		等待(1, 无视条件);
		事件玩家.R -= 1;
	}
}

