		禁用 创建效果(所有玩家(所有队伍), 环, 颜色(玫红), 全局.P[15], 1.200, 可见);
		禁用 全局.P[16] = 矢量(-39.368, 5.280, -11.240);
		禁用 创建效果(所有玩家(所有队伍), 环, 颜色(玫红), 全局.P[16], 1.200, 可见);
	}
}

