		根据条件跳过(全局.G < 3, 2);
		创建HUD文本(事件玩家, 字符串("{0} / {1}", 事件玩家.G[3], 全局.C[3]), 无, 自定义字符串("Boxes with yellow lids"), 左边, 3, 颜色(黄色), 颜色(白色), 颜色(黄色), 可见和字符串,
			始终不可见);
		事件玩家.E[3] = 上一个文本ID;
		根据条件跳过(全局.G < 4, 2);
		创建HUD文本(事件玩家, 字符串("{0} / {1}", 事件玩家.G[4], 全局.C[4]), 无, 自定义字符串("Plant Planted in a Large Earthen Pots"), 左边, 4, 颜色(绿色), 颜色(白色), 颜色(
			绿色), 可见和字符串, 始终不可见);
		事件玩家.E[4] = 上一个文本ID;
		根据条件跳过(全局.G < 5, 2);
		创建HUD文本(事件玩家, 字符串("{0} / {1}", 事件玩家.G[5], 全局.C[5]), 无, 自定义字符串("Alcoholic Beverage Dispensers"), 左边, 5, 颜色(蓝色), 颜色(白色), 颜色(蓝色),
			可见和字符串, 始终不可见);
