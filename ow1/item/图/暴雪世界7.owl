规则("传送操作--通用")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		全部;
	}

	条件
	{
		已重生(事件玩家) == 真;
		按钮被按下(事件玩家, 按钮(蹲下)) == 真;
		按钮被按下(事件玩家, 按钮(跳跃)) == 假;
		按钮被按下(事件玩家, 按钮(装填)) == 假;
		按钮被按下(事件玩家, 按钮(技能2)) == 假;
		按钮被按下(事件玩家, 按钮(终极技能)) == 假;
	}

	动作
	{
		If(相距距离(所选位置(事件玩家), 全局.P[1]) <= 2);
			传送(事件玩家, 全局.P[2]);
		Else If(相距距离(所选位置(事件玩家), 全局.P[2]) <= 2);
			传送(事件玩家, 全局.P[1]);
		Else If(相距距离(所选位置(事件玩家), 全局.P[3]) <= 2);
			传送(事件玩家, 全局.P[4]);
		Else If(相距距离(所选位置(事件玩家), 全局.P[4]) <= 2);
			传送(事件玩家, 全局.P[3]);
		Else If(相距距离(所选位置(事件玩家), 全局.P[5]) <= 2);
			传送(事件玩家, 全局.P[6]);
		Else If(相距距离(所选位置(事件玩家), 全局.P[6]) <= 2);
			传送(事件玩家, 全局.P[5]);
		Else If(相距距离(所选位置(事件玩家), 全局.P[7]) <= 2);
			传送(事件玩家, 全局.P[8]);
		Else If(相距距离(所选位置(事件玩家), 全局.P[8]) <= 2);
			传送(事件玩家, 全局.P[7]);
		Else If(相距距离(所选位置(事件玩家), 全局.P[9]) <= 2);
			传送(事件玩家, 全局.P[10]);
		Else If(相距距离(所选位置(事件玩家), 全局.P[10]) <= 2);
			传送(事件玩家, 全局.P[9]);
			禁用 Else If(相距距离(所选位置(事件玩家), 全局.P[11]) <= 2);
			禁用 传送(事件玩家, 全局.P[12]);
			禁用 Else If(相距距离(所选位置(事件玩家), 全局.P[12]) <= 2);
			禁用 传送(事件玩家, 全局.P[11]);
	}
}