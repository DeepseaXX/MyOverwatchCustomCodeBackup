{
	事件
	{
		持续 - 每名玩家;
		双方;
		全部;
	}

	条件
	{
		已重生(事件玩家) == 真;
		按钮被按下(事件玩家, 按钮(蹲下)) == 真;
		按钮被按下(事件玩家, 按钮(跳跃)) == 假;
		按钮被按下(事件玩家, 按钮(装填)) == 假;
		按钮被按下(事件玩家, 按钮(技能2)) == 假;
		按钮被按下(事件玩家, 按钮(终极技能)) == 假;
		(当前地图 == 地图(万圣节艾兴瓦尔德) || 当前地图 == 地图(艾兴瓦尔德)) == 真;
		
	}

	动作
	{
		禁用 If(相距距离(所选位置(事件玩家), 矢量(0, 0, 0)) <= 2);
